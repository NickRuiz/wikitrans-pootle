#: web/server.py:57
#, python-format
msgid "Login for %s"
msgstr "ཐོ་འགོད %s་།"

#: web/server.py:91
msgid "Cancel this action and start a new session"
msgstr "མངགས་བཀོད་འདི་མེད་པར་བཟོས་ནས་ཁ་བརྡ་གསར་བ་ཞིག་གི་མགོ་རྩོམ།"

#: web/server.py:92
msgid "Instead of confirming this action, log out and start from scratch"
msgstr "ལས་ཀ་འདི་དང་ལེན་བྱ་བ་ལས་ཕྱིར་བུད་ནས་གསར་བ་ཞིག་མགོ་རྩོམ།"

#: web/server.py:97
msgid "Exit application"
msgstr "མཉེན་ཆས་འདི་ལས་ཕྱིར་འབུད།"

#: web/server.py:98
msgid "Exit this application and return to the parent application"
msgstr "མཉེས་ཆས་འདི་ལས་ཕྱིར་བུད་ནས་སྔོན་མའི་མཉེས་ཆས་གཙོ་བོར་ལོག"

#: web/server.py:105
msgid ", please confirm login"
msgstr "ཐོ་འགོད་བྱས་པར་ཡང་བསྐྱར་དུ་དང་ལེན་བྱ་རོགས།"

#: web/server.py:110
msgid "Login"
msgstr "ནང་དུ་འཛུལ།"

#: web/server.py:123
msgid "Username:"
msgstr "བཀོལ་མིང་།"

#: web/server.py:125
msgid "Password:"
msgstr "གསང་ཚིག"

#: web/server.py:130
msgid "Language:"
msgstr "སྐད་རིགས།"

#: web/server.py:551 web/server.py:642
msgid "Select Application"
msgstr "མཉེན་ཆས་འདེམས་གསེས།"

#: web/server.py:637
msgid "Logout"
msgstr "ཕྱིར་འབུད།"

#: web/server.py:639
msgid "Exit"
msgstr "ཕྱིར་བུད།"

#: web/session.py:191 web/session.py:344 web/session.py:350
#, python-format
msgid "logged in as <b>%s</b>"
msgstr "<b>%s</b> ལྟར་དུ་ནང་ལ་འཛུལ།"

#: web/session.py:193
msgid "couldn't add new session"
msgstr "ཁ་བརྡ་གསར་བ་ཞིག་གི་མགོ་རྩོམ་ཐུབ་མ་སོང་།"

#: web/session.py:214
msgid "logged out"
msgstr "ཕྱིར་དོན།"

#: web/session.py:234
msgid "connected"
msgstr "སྦྲེལ།"

#: web/session.py:269
msgid "exited parent session"
msgstr "གླེང་མོལ་སྔོན་མ་ལས་ཕྱིར་བུད།"

#: web/session.py:314
msgid "need to define the users or userprefs entry on the server config"
msgstr ""
"བཀོལ་མི་དང་བཀོལ་མིའི་དགའ་ཕྱོགས་གཏན་འཁེལ་བྱས་ནས་དེ་དག་ཞབས་ཞུའི་སྒྲིག་གཞི་ནང་ད"
"ུ་འཇོག"

#: web/session.py:321 web/session.py:453
#, python-format
msgid "user does not exist (%r)"
msgstr "བཀོལ་མི་ (%r) ལྟ་བུ་ཞིག་མི་འདུག"

#: web/session.py:362
msgid "invalid username and/or password"
msgstr "བཀོལ་མིང་ངམ་ཡང་ན་གསང་ཚིག་ལ་ནོར་འཁྲུལ་འདུག"

#: web/session.py:433 web/session.py:437
msgid "failed login confirmation"
msgstr "ནང་འཛུལ་དང་ལེན་བྱེད་མ་ཐུབ།"

#: widgets/form.py:79 widgets/thumbgallery.py:292
msgid "No"
msgstr "མིན།"

#: widgets/form.py:80 widgets/thumbgallery.py:293
msgid "Yes"
msgstr "རེད།"

#: widgets/thumbgallery.py:233 attachments.py:348
msgid "remove"
msgstr "ཕྱིར་དབྱུངས།"

#: widgets/thumbgallery.py:289 attachments.py:322
msgid "(no attachment)"
msgstr "ཞོར་བཏགས་ཀྱི་ཡིག་ཆ་མེད།"

#: attachments.py:70
msgid "attachment is too large"
msgstr "ཞོར་གཏོགས་ཡིག་ཆ་ཆེ་སོང་།"

#: attachments.py:349
msgid "remove this attachment"
msgstr "ཟུར་གཏོགས་ཀྱི་ཡིག་ཆ་ཕྱིར་དཔྱུངས།"

#: attachments.py:350
msgid "restore"
msgstr "ཕྱིར་དཔྱུངས།"

#: attachments.py:351
msgid "this attachment has been removed. click here to restore it"
msgstr "ཞོར་བཏགས་ཀྱི་ཡིག་ཆ་ཕྱིར་དཔྱུངས་ཟིན། འདི་རྡེག་ནས་སླར་གསོ་བྱ།"

#: attachments.py:362
msgid "attach a file"
msgstr "ཡིག་ཆ་ཞིག་ཟུར་དུ་བཏགས།"
