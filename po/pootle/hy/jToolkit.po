#: web/server.py:57
#, python-format
msgid "Login for %s"
msgstr "%s-ի մուտք"

#: web/server.py:91
msgid "Cancel this action and start a new session"
msgstr "Դադարեցնել այս գործողությունը և նոր շրջան սկսել"

#: web/server.py:92
msgid "Instead of confirming this action, log out and start from scratch"
msgstr "Այս գործողությունը հաստատելու փոխարեն դուրս գալ և նորից սկսել"

#: web/server.py:97
msgid "Exit application"
msgstr "Ծրագիրը լքել"

#: web/server.py:98
msgid "Exit this application and return to the parent application"
msgstr "Լքել այս ծրագիրը և վերադառնալ ծնող ծրագրին"

#: web/server.py:105
msgid ", please confirm login"
msgstr "խնդրեմ՝ հաստատեք մուտքը"

#: web/server.py:110
msgid "Login"
msgstr "Մուտք"

#: web/server.py:123
msgid "Username:"
msgstr "Կոչական՝"

#: web/server.py:125
msgid "Password:"
msgstr "Ծածկագիր՝"

#: web/server.py:130
msgid "Language:"
msgstr "Լեզու՝"

#: web/server.py:551 web/server.py:642
msgid "Select Application"
msgstr "Ընտրել ծրագիր"

#: web/server.py:637
msgid "Logout"
msgstr "Ելք"

#: web/server.py:639
msgid "Exit"
msgstr "Լքել"

#: web/session.py:191 web/session.py:344 web/session.py:350
#, python-format
msgid "logged in as <b>%s</b>"
msgstr "գրանցված որպես՝ <b>%s</b>"

#: web/session.py:193
msgid "couldn't add new session"
msgstr "շրջան ավելացնել չի հաջողվում"

#: web/session.py:214
msgid "logged out"
msgstr "դուրս եկանք"

#: web/session.py:234
msgid "connected"
msgstr "միացած"

#: web/session.py:269
msgid "exited parent session"
msgstr "լքեցինք ծնող շրջանը"

#: web/session.py:314
msgid "need to define the users or userprefs entry on the server config"
msgstr "անհրաժեշտ է սերվերի քոնֆիգում users կամ userprefs գրառում նշել"

#: web/session.py:321 web/session.py:453
#, python-format
msgid "user does not exist (%r)"
msgstr "յուզերը գոյեւթյուն չունի (%r)"

#: web/session.py:362
msgid "invalid username and/or password"
msgstr "սխալ կոչական և/կամ ծածկագիր"

#: web/session.py:433 web/session.py:437
msgid "failed login confirmation"
msgstr "մուտքի հաստատումը ձախողվեց"

#: widgets/form.py:79 widgets/thumbgallery.py:292
msgid "No"
msgstr "Ոչ"

#: widgets/form.py:80 widgets/thumbgallery.py:293
msgid "Yes"
msgstr "Այո"

#: widgets/thumbgallery.py:233 attachments.py:348
msgid "remove"
msgstr "հեռացնել"

#: widgets/thumbgallery.py:289 attachments.py:322
msgid "(no attachment)"
msgstr "(հավելված չկա)"

#: attachments.py:70
msgid "attachment is too large"
msgstr "հավելվածը շատ մեծ է"

#: attachments.py:349
msgid "remove this attachment"
msgstr "հավելվածը հեռացնել"

#: attachments.py:350
msgid "restore"
msgstr "վերականգնել"

#: attachments.py:351
msgid "this attachment has been removed. click here to restore it"
msgstr "հավելվածը հեռացվել է։ Սեղմե՛ք այստեղ այն վերականգնելու համար"

#: attachments.py:362
msgid "attach a file"
msgstr "ֆայլ հավելել"
